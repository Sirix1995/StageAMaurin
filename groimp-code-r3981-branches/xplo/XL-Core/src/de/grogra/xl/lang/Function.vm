package de.grogra.xl.lang;

/**
 * Instances of <code>${Src}To${Dest}</code> represent functions
 * which take <code>$src</code>s as input and return values of
 * type <code>$dest</code>. Such instances are created by
 * lambda expressions of the XL programming language, e.g.,
#if ($src == "void")
 * <code>${Src}To${Dest} f = $src => $dest</code>
 * <i>some expression</i><code>;</code>.
#else
 * <code>${Src}To${Dest} f = $src x => $dest</code>
 * <i>some expression containing x</i><code>;</code>.
#end
 *
 * @author Ole Kniemeyer
 */
public interface ${Src}To${Dest}${sig}
{
#if ($src == "void")
	/**
	 * Computes the value of this function.
#if ($dest != "void")
	 * 
	 * @return function value
#end
	 */
	$dest evaluate$Dest ();
#else
	/**
	 * Computes the value of this function at <code>x</code>.
	 * 
	 * @param x where the function is to be evaluated
#if ($dest != "void")
	 * @return function value at <code>x</code>
#end
	 */
	$dest evaluate$Dest ($src x);
#end
}
#end
