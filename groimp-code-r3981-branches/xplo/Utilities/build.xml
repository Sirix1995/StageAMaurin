<project name="Utilities" default="compile">
  <description>
    Ant build file for the utilities package
  </description>

  <import file="../Build/buildproject.xml"/>

  <target name="-app"/>

  <target name="-projects">
    <antcall target="-addproject">
      <param name="proj" value="XL-Core"/>
